<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link rel="apple-touch-icon" sizes="57x57" href="/assets/touch/apple-touch-icon-57x57.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="60x60" href="/assets/touch/apple-touch-icon-60x60.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="72x72" href="/assets/touch/apple-touch-icon-72x72.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="76x76" href="/assets/touch/apple-touch-icon-76x76.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="114x114" href="/assets/touch/apple-touch-icon-114x114.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="120x120" href="/assets/touch/apple-touch-icon-120x120.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="144x144" href="/assets/touch/apple-touch-icon-144x144.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="152x152" href="/assets/touch/apple-touch-icon-152x152.png?v={squarespace.template-revision}" />
        <link rel="apple-touch-icon" sizes="180x180" href="/assets/touch/apple-touch-icon-180x180.png?v={squarespace.template-revision}" />
        <link rel="icon" type="image/png" href="/assets/touch/android-chrome-192x192.png?v={squarespace.template-revision}" sizes="192x192" />
        <meta name="msapplication-square70x70logo" content="/assets/touch/smalltile.png?v={squarespace.template-revision}" />
        <meta name="msapplication-square150x150logo" content="/assets/touch/mediumtile.png?v={squarespace.template-revision}" />
        <meta name="msapplication-wide310x150logo" content="/assets/touch/widetile.png?v={squarespace.template-revision}" />
        <meta name="msapplication-square310x310logo" content="/assets/touch/largetile.png?v={squarespace.template-revision}" />
        {squarespace-headers}
    </head>
    <body>
        <nav class="navi js-navi -wrap -fzero -pagepad" data-content-field="navigation-navi">
            <div class="navi__header">
                <div class="icon icon--ex js-controller--navi">
                    <span class="ex1"></span>
                    <span class="ex2"></span>
                </div>
            </div>
            <squarespace:navigation navigationId="navi" template="navi" />
            <ul class="navi__social social">
                {.repeated section website.socialAccounts}
                    <li class="social__li -column">
                        <a href="{profileUrl}" class="social__a m -light" target="_blank">{serviceName}</a>
                    </li>
                {.end}
            </ul>
        </nav>
        <menu class="contact js-contact -wrap -fzero -pagepad">
            <div class="contact__header">
                <div class="icon icon--ex js-controller--contact">
                    <span class="ex1"></span>
                    <span class="ex2"></span>
                </div>
            </div>
            <div class="contact__form js-contact-form">
                <div class="h2 -light">Contact</div>
                <div class="m -light -bump3">\ {websiteSettings.contactPhoneNumber}</div>
                <div class="m -light">\ <a href="mailto:newbiz@struck.com" target="_blank">newbiz@struck.com</a></div>
                <div class="m -light">\ <a href="mailto:press@struck.com" target="_blank">press@struck.com</a></div>
                <div class="m -color">\ Locations</div>
                <input class="inp js-contact-field -bump3" type="text" name="text-yui_3_17_2_1_1513626979285_41600" placeholder="*Name" />
                <input class="inp js-contact-field -bump3" type="text" name="text-yui_3_17_2_1_1513626979285_24222" placeholder="Company" />
                <input class="inp js-contact-field -bump3" type="text" name="email-yui_3_17_2_1_1513626979285_25400" placeholder="*Email" />
                <input class="inp js-contact-field -bump3" type="text" name="phone-yui_3_17_2_1_1513626979285_43483" placeholder="Phone" />
                <textarea class="inp inp--txt js-contact-field -bump3" name="textarea-yui_3_17_2_17_1513626979285_3714" placeholder="*Message"></textarea>
                <div class="m -light -bump">*Required</div>
                <div class="-bump6">
                    <div class="-column -column--2of4">
                        <button class="btn btn--light js-contact-submit">Submit</button>
                    </div>
                    <div class="-column -column--2of4 -text--right">
                        <div class="m -grey -curse js-contact-reset">Reset Form</div>
                    </div>
                </div>
            </div>
        </menu>
        {@|apply header.block}
        <main class="page js-page -fzero -pagepad" data-content-field="main-content" data-website-id="{website.id}" {.section collection}data-collection-id="{collection.id}" data-collection-title="{.if collection.homepage}{website.siteTitle}{.or}{collection.title} – {website.siteTitle}{.end}"{.end} {.section item}data-item-id="{item.id}" data-item-title="{item.title} – {collection.title} – {website.siteTitle}"{.end}>
            {.equal? collection.typeName "page"}
                {@|apply page.block}
            {.or}
                {squarespace.main-content}
            {.end}
        </main>
        {@|apply footer.block}
        {@|apply intro.block}
        <script type="text/javascript" src="/scripts/app.js?v={squarespace.template-revision}"></script>
        {@|apply google.block}
        {squarespace-footers}
    </body>
</html>
