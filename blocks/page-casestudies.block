<div class="page__mast -wrap">
    <div class="h2 -mast">
        <squarespace:block-field id="module-case-studies-masthead" columns="12" />
    </div>
    {@|apply module-filters.block}
</div>
<div class="page__grid grid -wrap -exp">
    {.repeated section items}
        <a class="grid__item grid__item--2up -column" href="{fullUrl}">
            <div class="grid__image js-lazy-image js-aspect -cover" data-img-src="{assetUrl}" data-variants="{systemDataVariants}" data-original="{originalSize}"></div>
            <div class="grid__info">
                <div class="grid__title m -light -caps">{title}</div>
                <div class="grid__meta">
                    {.repeated section tags}
                        <div class="filters__item p -light -column">{@}</div>
                    {.end}
                </div>
            </div>
        </a>
    {.end}
</div>
<div class="page__modules">
    {@|apply module-reintroduce.block}
    <div class="-wrap -expt">
        {@|apply module-instagram.block}
        {@|apply module-contact.block}
    </div>
</div>
